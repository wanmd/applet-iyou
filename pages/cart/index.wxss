    display: flex;
    align-items: center;
    justify-content: space-between;
}

.car-goods-item .goods-info--b .price-info {
    display: flex;
    color: #ECBD75;
}

.car-goods-item .goods-info--b .price-info .text {
    color: #ACACAC;
    padding-right: 10rpx;
    font-size: 22rpx;
    font-family: PingFangSC-Medium, PingFangSC;
    font-weight: normal;
}

.car-goods-item .goods-info--b .icon-price {
    font-size: 20rpx;
