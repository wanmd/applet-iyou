    background-repeat: no-repeat;
}

.noticeBox {
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    padding: 8rpx 30rpx;
    font-size: 24rpx;
    color: #666666;
    background: #FFE200;
}

.notice-col {
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    -webkit-flex: 1;
    -ms-flex: 1;
    flex: 1;
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    height: 40rpx;
    overflow: hidden;
    line-height: 40rpx;
    background: #FFE200;
}

.notice-li {
    width: 100%;
    position: relative;
    overflow: hidden;
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    -webkit-flex: 1;
    -ms-flex: 1;
    flex: 1;
    transition: .8s all;
    line-height: 40rpx;
    text-align: left;
    height: 40rpx;
    padding-left: 10rpx;
}

.notice__it {
    white-space: nowrap;
    position: absolute;
    top: 0;
    height: 40rpx;
    overflow: hidden;
    line-height: 40rpx;
    font-size: 24rpx;
    color: #666666;
}

