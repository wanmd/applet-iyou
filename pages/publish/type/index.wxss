.panel {
    position: fixed;
    left: 0;
    bottom: 0rpx;
    width: 100%;
}

.title {
    font-size: 56rpx;
    text-align: center;
}

.items {
    margin-top: 88rpx;
}

.items-1 {
    overflow: hidden;
    margin: 80rpx auto 0 auto;
    width: 120rpx;
    font-weight: bold;
}

.items-2 {
    overflow: hidden;
    margin: auto;
    width: 350rpx;
    font-weight: bold;
}

.items-3 {
    width: 524rpx;
    overflow: hidden;
    margin: 80rpx auto 0 auto;
}

.items-1 .item {
    width: 120rpx;
}

.items-2 .item {
    width: 120rpx;
    margin-left: 82rpx;
}

.items-2 .item:first-of-type {
    margin-left: 0rpx;
}

.items-3 .item {
    width: 120rpx;
    margin-left: 82rpx;
}

.items-3 .item:first-of-type {
    margin-left: 0rpx;
}

.item {
    width: 110rpx;
    text-align: center;
    color: #434759;
    white-space: nowrap;
}

.item .imgBox {
    width: 88rpx;
    height: 108rpx;
    margin: 0rpx auto;
    display: flex;
    justify-items: center;
    align-items: center;
}

.item image {
    width: 88rpx;
    display: block;
    margin: 0rpx auto;
}

.line1 .item image {
    width: 68rpx;
}


/* 
