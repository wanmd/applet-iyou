page {
  background-color: #fff;
}
.level2 {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 282rpx;
  height: 68rpx;
  /* background: #FFFFFF; */
  border: 2rpx solid #F7F7F7;
  font-size: 24rpx;
  line-height: 32rpx;
  color: #333333;
  margin-right: 40rpx;
  margin-bottom: 8rpx;

}
.bottom {
  position: fixed;
  bottom: 0rpx;
  width: 100%;
  padding-top: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  /* border-top: 1rpx solid #F7F7F7; */
  z-index: 9;
  background-color: #fff;
}
.save {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 400rpx;
  height: 96rpx;
  background: #FFE200;
  border-radius: 48rpx;
  font-size: 32rpx;
  line-height: 42rpx;
  color: #333333;
  margin-bottom: 24rpx;
}
.edit {
  width: 400rpx;
  background-color: transparent;
  text-align: center;
  font-size: 30rpx;
  line-height: 44rpx;
  color: #333333;
  padding: 20rpx;
  margin-bottom: 100rpx;
}