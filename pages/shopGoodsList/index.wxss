page {
  --tab-active-text-color: #ff6c1e;
  font-size: 24rpx;
  height: 100vh;
}
.h-100rpx {
  height: 100rpx;
}
.sidebar-click {
  background: #fff;
  font-weight: 600;
  color: #535353;
}
.sidebar-click text{
  font-size: 28rpx !important;
  color: #AB00FF;
  /* font-weight: 600 !important; */
}
.sidebar-click:before {
  content: "";
  display: block;
  width: 12rpx;
  height: 100rpx;
  background: #AB00FF;
  position: absolute;
  left: 0rpx;
}

/* 右边内容 */
 .partition-wrap {
  overflow-y: scroll;
}
page {
  width: 100%;
  height: 100%;
}

.container {
  width: 100%;
  height: 100%;
}

.nav {
  height: 70rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 70rpx;
  font-size: 16px;
  white-space: nowrap;
  z-index: 99;
}

.nav-item {
  display: inline-block;
  text-align: center;
  margin-right: 20rpx;
  padding: 0 20rpx;
  border-radius: 6rpx;
  height: 44rpx;
  background: #F7F7F7;
  border-radius: 22rpx;
  font-size: 24rpx;
  line-height: 44rpx;
  color: #0B0B0B;
}

.nav-item.active {
  border: 1px solid #AB00FF;
  color: #AB00FF;
  background: rgba(171, 0, 255, 0.08);
}

.tab-box {
  box-sizing: border-box;
}

.navbar {
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
  flex-shrink: 0;
}

.navbar-action-wrap {
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  align-items: center;
  position: absolute;
  left: 10px;
  z-index: 11;
  line-height: 1;
  padding-top: 4px;
  padding-bottom: 4px;
}
