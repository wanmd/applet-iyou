<view wx:if="{{true}}">
  <view class="d-f goodsBox borderb-1-solid-F7F7F7 ml-48 mr-48 pt-20 pb-32" data-path="/pages/packagesGoods/goodsDetail/index?goodsId={{tabItem.id}}" bind:tap="handleJump">
    <view class="goodsImg d-f w-180 h-180 mr-16">
      <image style="width:180rpx;height:180rpx" src="../../../../../assets/images/default-jj.png"></image>
    </view>
    <view class="d-f fxd-c jc-sb">
      <view>
        <view class="c-666 fz-28 tov-e-1 mb-8 lh-46">Daily X Brocade 双丝光纯棉短袖T恤</view>
        <view class="d-f fxw-w">
          <!-- <block wx:for="{{tabItem.tagList}}" wx:for-item="tagList" wx:for-index="idx" wx:key="idx">
            <view class="tagListBox">{{tagList}}</view>
          </block> -->
          <view class="tagListBox">产品标签6字</view>
        </view>
      </view>
      
      <view class="saleBox d-f ai-fe">
        <text class="c-000 fz-20 lh-28">￥</text>
        <text class="c-000 fz-28 lh-32">2451</text>
        <text class="c-AFAFAF fz-20 lh-22 ml-20 td-l">￥3451</text>
      </view>
    </view>
  </view>
  <view class="groupsWrap plr-120 mt-58">
    <view class="d-f jc-sb ai-c">
      <view class="w-100rpx h-100rpx mr-46 bdrs-50p ov-h">
          <image src="{{userInfo.avatar}}" class="w-100rpx h-100rpx" />
      </view>
      <view class="w-64 h-58 mr-46 bdrs-50p ov-h">
          <image src="/assets/images/index/groupwith@2x.png" class="w-64 h-58" />
      </view>
      <view class="w-100rpx h-100rpx bdrs-50p ov-h">
          <image src="../../../../../assets/images/default-jj.png" class="w-100rpx h-100rpx" />
      </view>
      
    </view>
    <view class="d-f fxd-c jc-c ai-c">
      <view class="c-AFAFAF fz-24 mt-24 mb-46">TA选的规格：  拼主选择的规格文字</view>
      <view class="c-FF0000 fz-28 lh-44">
        <timer time="{{20000}}" color="#333333" />后结束
      </view>
      <view class="mt-18 mb-42 fz-24 lh-44 c-3A3A3A">还差 1 位好友即可拼单成功，快去邀请TA吧！</view>
      <button hover-class="none" class='invite' open-type='share'>
        邀请好友拼单
      </button>
      <view class="mt-56 fz-24 lh-44 c-3A3A3A" bindtap="navToOrderDetail">查看订单详情 ></view>
    </view>
  </view>
</view>
<!-- 拼团成功状态 -->
<view wx:if="{{true}}">
  <view class="groupsWrap plr-120 mt-58">
    <view class="ta-c pos-r">
        <text class="info">拼单成功</text>
        <image src="/assets/images/iyou_user/success@2x.png" class="w-424 h-236" />
    </view>
    <view class="d-f jc-sb ai-c">
      
      <view class="w-100rpx h-100rpx mr-46 bdrs-50p ov-h">
          <image src="{{userInfo.avatar}}" class="w-100rpx h-100rpx" />
      </view>
      <view class="w-64 h-58 mr-46 bdrs-50p ov-h">
          <image src="/assets/images/index/groupwith@2x.png" class="w-64 h-58" />
      </view>
      <view class="">
        <image src="{{userInfo.avatar}}" class="w-100rpx h-100rpx bdrs-50p" />
        <image src="../../../../../assets/images/default-jj.png" class="w-100rpx h-100rpx bdrs-50p ml--50" />
      </view>
      
    </view>
    <view class="d-f fxd-c jc-c ai-c mt-60">
      <view class="invite">去看看更多优物</view>
      <view class="mt-56 fz-24 lh-44 c-3A3A3A">查看订单详情 ></view>
    </view>
  </view>
</view>
  