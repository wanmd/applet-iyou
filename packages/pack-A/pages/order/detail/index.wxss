/* //上拉运动 */
@keyframes sliup_1 {
  from {
    opacity: 0;
    transform: translateY(120%);
  }
  to {
    opacity: 1;
    transform: translateY(0%);
  }
}

.sliup_1 {
  display: block;
  animation: sliup_1 .35s ease-in both;
}

@keyframes slidown_1 {
  from {
    opacity: 1;
    transform: translateY(0%);
  }
  to {
    opacity: 0;
    transform: translateY(120%);
  }
}

.slidown_none {
  display: none !important;
}

.slidown_1 {
  /* // display: none; */
  animation: slidown_1 .35s ease-in both;
}

.show_h {
  display: block;
  animation: show_h .35s ease-in both;
}

@keyframes show_h {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}

.show_l {
  display: block;
  animation: show_l .35s ease-in both;
}

@keyframes show_l {
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
    display: none;
  }
}







