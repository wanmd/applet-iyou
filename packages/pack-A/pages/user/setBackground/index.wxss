/* pages/userInfo/setBackground/index.wxss */
@import '../../../../../utils//wxss/index.wxss';

.image{
  display:block;
  width:336rpx;
  height:336rpx;
}
.module{
  margin-top:40rpx;
}

.module .title{
  font-size:24rpx;
  font-weight:bold;
  position:relative;
  padding-left:30rpx;
}
.module .title::before{
  position:absolute;
  content:'';
  width:10rpx;
  height:10rpx;
  background:#333;
  border-radius:50%;
  left:0;
  top:50%;
  transform:translateY(-50%);
}

.module .body{
  padding:0rpx 70rpx;
  margin-top:30rpx;
}