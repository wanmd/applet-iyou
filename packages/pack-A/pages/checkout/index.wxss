page{
  background:rgba(247,247,247,1);
  padding-bottom:140rpx;
}

.edit-line{
  text-align:right;
  position:fixed;
  left:0;
  top:0;
  width:100%;
  z-index:2;
}

.user-line{
  padding:30rpx 0rpx;
  position:relative;
}
.avatar{
  height:48rpx;
  width:48rpx;
  display:block;
  border-radius:50%;
}
.user-info{
  position:absolute;
  left:60rpx;
  top:50%;
  transform:translateY(-50%);
  font-weight:500;
  width: 300rpx;
  white-space:nowrap;overflow:hidden;text-overflow:ellipsis;
}

.cart-list{
  background-color: #fff;
}
.cart-list > .item{
  background:#fff;
  margin-top:24rpx;
}
.cart-list > .item:first-child,
.cart-list > .item:first-of-type{
  margin-top:0rpx;
}
.goods-list .goods{
  padding:32rpx 0rpx 32rpx 0rpx;
  position:relative;
  border-bottom:1px solid #f7f7f7;
}


.goods-list .goods .checkbox{
  position:absolute;
  left:0;
}
.goods-list .goods .cover{
  display:block;
  width:216rpx;
  height:216rpx;
}

.content{
  position:absolute;
  left:245rpx;
  right:0rpx;
  top:0rpx;
  height:100%;
  /*white-space: nowrap;*/
  /*flex-direction: row;*/
  /*flex-wrap: nowrap;*/
  display: flex;
  align-content: space-between;
  flex-wrap: wrap;
}

.goods-name{
  color:#666666;
  font-size:30rpx;
}

.goods-price{
  /*position:absolute;
  left:0rpx;
  bottom:0rpx;*/
