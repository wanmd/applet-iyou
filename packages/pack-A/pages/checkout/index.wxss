}
.goods-price,
.goods-price1,
.goodsNum{
  color: #656565;
  width: 100%;
  display: flex;
}

.goods-price view,
.goods-price1 view,
.goodsNum view{
  flex: 1;
  display: inline-block;
  text-align: right;
}
.goodsNum .num{
  text-align: right;
}
.goods-price view{
  color: #EAB96D;
}
.goods-price .num,
.goods-price1 .num{
  font-size: 30rpx;
  font-weight:500;
}

.counter{
  position:absolute;
  right:0rpx;
  bottom:0rpx;
}

.cart-bottom{
  position:fixed;
  left:0rpx;
  bottom:0rpx;
  
  width:100%;
  background:#fff;
  z-index: 9;
}

.cart-bottom .wrap{
  height:96rpx;
  padding: 6rpx 0;
  box-sizing: border-box;
  display: flex;
  align-content: center;
  flex-wrap: wrap;
}

.wrap__1,
.wrap__2{
  vertical-align: middle;
  font-size: 24rpx;
  color: #737373;
  padding-right: 320rpx;
  display: flex;
  width: 100%;
  line-height: 36rpx;
}

.cart-bottom .hj{
}
.cart-bottom .flex__1{
  flex: 1;
  text-align: right;
}

