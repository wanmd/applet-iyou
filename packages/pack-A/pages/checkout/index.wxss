.address_item1,
.address_item2{
  padding-left: 70rpx;
  position: relative;
  width: 690rpx;
  box-sizing: border-box;
}
.address_item1>.ai__address,
.address_item2>.ai__address,
.address_item1>.item-icon,
.address_item2>.item-icon{
  position: absolute;
  top: 50%;
}
.address_item1>.item-icon,
.address_item2>.item-icon{
  left: 0;
  margin-top: -20rpx;
}
.address_item1>.ai__address,
.address_item2>.ai__address{
  height: 32rpx;
  right: 0;
  padding: 30rpx 10rpx;
  margin-top: -50rpx;
  z-index: 10;
}
.address_item1>.ai__address{
  margin-top: -30rpx;
}

