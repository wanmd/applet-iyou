page{
  padding-bottom:120rpx;
}
.p-l-32{
padding-left:32rpx;
}
.p-r-32{
padding-right:32rpx;
}
.module-title{
  font-size:32rpx;
  position:relative;
}
.module-title::before{
  content:'';
  position:absolute;
  height:100%;
  width:10rpx;
  top:0rpx;
  left:0rpx;
  background:#FFE200;
}

.idcard-upload .item{
  width:336rpx;
  height:336rpx;
  position:relative;
}
.idcard-upload .item .wrap{
  position:absolute;
  left:50%;
  top:50%;
  transform:translate(-50%, -50%);
  text-align:center;
}
.idcard-upload .item .wrap .el-image{
  display:block;
  width:228rpx;
  height: 154rpx;
}

.zhizhao .item{
  width:336rpx;
  height:460rpx;
}

.upload{
  background:#e7f3ff;
  border:1px dashed #1890FF;
  position:relative;
  text-align:center;
  white-space:nowrap;
}
.upload .wrap {
  position:absolute;
  color:#999;
  font-size:24rpx;
}
.upload .wrap .icon{
  display:block;margin:auto;width:32rpx;height:32rpx;
}

.store .item{
  width:336rpx;
  height:224rpx;
}

